<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="invoice_big_animal_reg" language="groovy" pageWidth="842" pageHeight="610" orientation="Landscape" columnWidth="832" leftMargin="0" rightMargin="10" topMargin="0" bottomMargin="10" uuid="0e3d1ce5-54b9-4676-8053-29c780a28060">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<style name="Crosstab Data Text" hAlign="Center"/>
	<parameter name="USER_NAME" class="java.lang.String"/>
	<parameter name="fromdate" class="java.lang.String"/>
	<parameter name="todate" class="java.lang.String"/>
	<parameter name="terr_code" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
	'Data entry:' as activity_type,
	concat('Tag big an./მ.რ.პ. რეგისტრაცია (', taa.status, ')') as activity_name,
	to_char(taa.registration_date::date, 'YYYY-MM-DD') as activity_date,
	naits.get_label_text_per_value(vh.village_code,
	'VILLAGES',
	'ka_GE') as village,
	0 as holding_count,
	prices.price_per_hold as holding_price,
	0 as holding_total,
	count(taa.*) as animal_count,
	prices.price_per_act as animal_price,
	(prices.price_per_act * count(taa.*)) as animal_total,
	cast (0 as bigint) as doc_count,
	prices.price_per_doc as doc_price,
	0 as doc_total,
	$P{USER_NAME} as user_name,
	0 + (prices.price_per_act * count(taa.*)) + 0 as overall_total
from
	(
	select
		--distinct
 		va.dt_insert::date as registration_date,
		va.status,
		va.animal_id,
		va.animal_class,
		va.country,
		va.parent_id
	from
		naits.vanimal va
		--join naits.vsvarog_users vu on va.user_id=vu.object_id --and user_name!='ADMIN'

		where now() < va.dt_delete
		and ( va.animal_class = '1'
		or va.animal_class = '2'
		or va.animal_class = '12')/*small or big animals - price dependance*/
		and va.dt_insert between to_timestamp($P{fromdate} || ' 00:00:00', 'DD.MM.YYYY HH24:MI:SS') and to_timestamp($P{todate} || ' 59:59:00', 'DD.MM.YYYY HH24:MI:SS')
		and va.user_id = (
		select
			vu.object_id
		from
			naits.vsvarog_users vu
		where
			now() < vu.dt_delete
			and user_name = $P{USER_NAME})
		and not exists (
		select
			1
		from
			naits.vanimal vaa
		where
			vaa.animal_id = va.animal_id
			and vaa.dt_insert < to_timestamp($P{fromdate} || ' 00:00:00', 'DD.MM.YYYY HH24:MI:SS'))) taa
join naits.vholding vh on
	taa.parent_id = vh.object_id
left join (
	select
		*
	from
		naits.invoice_price
	where
		act_name = '3') prices on
	'1' = '1' /*'holding visit, big'*/
where
	now() between vh.dt_insert and vh.dt_delete
	and
	(case
		when length($P{terr_code})= 8 then vh.village_code = $P{terr_code}
		else
		case
			when length($P{terr_code})= 6 then vh.commun_code = $P{terr_code}
			else
			case
				when length($P{terr_code})= 4 then vh.munic_code = $P{terr_code}
				else
				case
					when length($P{terr_code})= 2 then vh.region_code = $P{terr_code}
					else 1 = 0 end end end end)
group by
	taa.registration_date,
	taa.status,
	vh.village_code,
	prices.price_per_hold,
	prices.price_per_act,
	prices.price_per_doc;]]>
	</queryString>
