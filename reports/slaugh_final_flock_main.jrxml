<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="slaugh_final_flock_main" language="groovy" pageWidth="541" pageHeight="842" whenNoDataType="NoDataSection" columnWidth="501" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="1ce36bd2-96f5-442e-b778-f3a46c2d4273">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="287"/>
	<parameter name="OBJ_ID" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="path" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
	va.flock_id,
	(date_part('day', now()) - date_part('day', va.registration_date))animal_day_diff,
	naits.translateCodeValue(va.animal_type,
	'FLOCK_ANIMAL_TYPE',
	'en_US')animal_breed,
	va.registration_date,
	vsf.slaughter_date,
	vh.pic,
	vhr.full_name
from
	naits.vflock va
join naits.vholding vh on
	va.parent_id = vh.object_id
join naits.vpost_slaught_form vsf on
	(vsf.parent_id = va.object_id and now() between vsf.dt_insert and vsf.dt_delete)
left join naits.vsvarog_link vsl on
	(vsl.link_obj_id_1 = vh.object_id
	and now() between vsl.dt_insert and vsl.dt_delete
	and vsl.link_type_id in (
	select
		object_id
	from
		naits.vsvarog_link_type vlt
	where
		now() < dt_delete
		and vlt.link_type = 'HOLDING_KEEPER' ))
left join naits.vholding_responsible vhr on
	(vsl.link_obj_id_2 = vhr.object_id
	and now() between vhr.dt_insert and vhr.dt_delete)
where
	now() between va.dt_insert and va.dt_delete
	and now() between vh.dt_insert and vh.dt_delete
	and va.object_id = $P{OBJ_ID};]]>
	</queryString>
	<field name="flock_id" class="java.lang.String"/>
	<field name="animal_day_diff" class="java.lang.Double"/>
	<field name="animal_breed" class="java.lang.String"/>
	<field name="registration_date" class="java.sql.Date"/>
	<field name="slaughter_date" class="java.sql.Date"/>
	<field name="pic" class="java.lang.String"/>
	<field name="full_name" class="java.lang.String"/>
	<variable name="DAYS_DIFFERENCE" class="java.lang.Integer">
		<variableExpression><![CDATA[new Integer((int)((new Date().getTime() - $F{registration_date}.getTime()) / (1000*60*60*24)))]]></variableExpression>
	</variable>
