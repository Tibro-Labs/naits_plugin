	</parameter>
	<queryString>
		<![CDATA[select va.animal_id, va.old_animal_id,
(date_part('year', now()) - date_part('year', va.birth_date))animal_age,
(date_part('day', now()) - date_part('day', va.birth_date))animal_day_diff,
naits.translateCodeValue(va.animal_class,'ANIMAL_CLASS','en_US')animal_breed,
va.country_old_id, va.country, va.birth_date, va.registration_date,
vsf.slaughter_date,
vhr.full_name,
vh.name
from naits.vanimal va join naits.vpost_slaught_form vsf on vsf.parent_id = va.object_id join naits.vholding vh on va.parent_id=vh.object_id join naits.vsvarog_link vsl on vsl.link_obj_id_1=vh.object_id
join naits.vholding_responsible vhr on vsl.link_obj_id_2=vhr.object_id join naits.vsvarog_link_type vlt on vsl.link_type_id = vlt.object_id
where now() between va.dt_insert and va.dt_delete
and now() between vh.dt_insert and vh.dt_delete
and now() between vsf.dt_insert and vsf.dt_delete
and now() between vsl.dt_insert and vsl.dt_delete
and now() between vhr.dt_insert and vhr.dt_delete
and now() between vlt.dt_insert and vlt.dt_delete
and va.status='SLAUGHTRD'
and vlt.link_type='HOLDING_KEEPER'
and va.object_id=$P{OBJ_ID}]]>
	</queryString>
