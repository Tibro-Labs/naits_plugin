	<property name="ireport.y" value="0"/>
	<parameter name="terr_code" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="USER_NAME" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fromdate" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="todate" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
	$P{USER_NAME} as username,
	to_char(vb.vacc_date, 'dd/mm/yyyy') as event_date,
	'Vaccination/ვაქცინაცია: ' || vb.vacc_code as code,
	va.animal_id as animal_id,
	vh.pic as holding_id,
	/*animal info: spec. breed, color, gender, birth date*/
	naits.get_label_text_per_value(va.animal_class,
	'ANIMAL_CLASS',
	'en_US') as animal_class,
	naits.get_label_text_per_value(va.animal_race,
	'ANIMAL_RACE',
	'en_US') as animal_race,
	naits.get_label_text_per_value(va.color,
	'COLOR',
	'en_US') as animal_color,
	naits.get_label_text_per_value(va.gender,
	'GENDER',
	'en_US') as animal_gender,
	va.status as animal_status,
	to_char(va.birth_date, 'dd/mm/yyyy') as animal_birth,
	to_char(va.death_date, 'dd/mm/yyyy') as animal_death,
	vh.name as holding_info,
	vh.village_name as village,
	to_char(vb.dt_insert, 'dd/mm/yyyy') as date_reg
from
	naits.vholding vh
join naits.vanimal va on
	va.parent_id = vh.object_id /* ADDITIONAL INFO */
join naits.vsvarog_link vl on
	(va.object_id = vl.link_obj_id_1
	and vl.link_type_id = (
	select
		object_id
	from
		naits.vsvarog_link_type
	where
		now() between dt_insert and dt_delete
		and link_type = 'ANIMAL_VACC_BOOK' ))
join naits.vvaccination_book vb on
	vb.object_id = vl.link_obj_id_2
where
	now() < vl.dt_delete
	and now() < va.dt_delete
	and now() < vb.dt_delete
	and vb.status = 'VALID'
	and va.status = 'VALID'
	and vb.dt_insert between to_timestamp($P{fromdate} || ' 00:00:00', 'DD.MM.YYYY HH24:MI:SS') and to_timestamp($P{todate} || ' 23:59:59', 'DD.MM.YYYY HH24:MI:SS')
	and ( ($P{USER_NAME} = '')
	or (vl.user_id = (
	select
		object_id
	from
		naits.vsvarog_users us
	where
		now() < us.dt_delete
		and us.status = 'VALID'
		and us.user_name = $P{USER_NAME}
	limit 1)))
	and vh.village_code like ($P{terr_code} || '%');]]>
	</queryString>
